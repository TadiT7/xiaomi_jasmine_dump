      Name="bug_report"
      Command="bugreport"
      Type="oneshot"
      Extension=".txt"/>
   <HLOSCommand
      Name="surface_flinger"
      Command="dumpsys SurfaceFlinger"
      Type="oneshot"
      Extension=".txt"/>

